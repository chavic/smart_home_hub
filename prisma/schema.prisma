// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model Event {
  devId   String
  i       Int
  tsMs    BigInt
  cap     String
  evt     String
  payload String

  @@id([devId, i])
}

model Command {
  devId   String
  i       Int    @id
  tsSent  BigInt
  cmd     String
  status  String
  result  String?
}
